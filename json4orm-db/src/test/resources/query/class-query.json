{
  query: "Class",
  filter: {
    and: [
	  {
	     name: "Math"
	  },
	  {
	    "classTeachers.teacher.firstName": {
	       in: ["Leonie","Kanya"]
	    } 
	  }
	]   
  },
  result: {
    Class: {
      properties: ["name"],
      classTeachers: { 
        teacher: ["firstName", "lastName","birthDate"]
      },
      classStudents: {
        properties: ["classStudentId", "score"],
        student: ["firstName", "lastName","birthDate"]
      }
    }  
  }
}